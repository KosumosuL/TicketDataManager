    <!--分页&过滤-->
    <servlet>
        <servlet-name>pageAdmin</servlet-name>
        <servlet-class>forFilter_Page.pageAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pageAdmin</servlet-name>
        <url-pattern>*.adminManage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>pageUser</servlet-name>
        <servlet-class>forFilter_Page.pageUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pageUser</servlet-name>
        <url-pattern>*.userManage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>filterStatis</servlet-name>
        <servlet-class>forFilter_Page.filterStatis</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>filterStatis</servlet-name>
        <url-pattern>*.filterStatis</url-pattern>
    </servlet-mapping>
    <!--管理界面-->
    <servlet>
        <servlet-name>userManage</servlet-name>
        <servlet-class>forAction.userManage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userManage</servlet-name>
        <url-pattern>*.servletuserManage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>adminManage</servlet-name>
        <servlet-class>forAction.adminManage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminManage</servlet-name>
        <url-pattern>*.servletadminManage</url-pattern>
    </servlet-mapping>
    <!--工作票导入-->
    <servlet>
        <servlet-name>ticketInput</servlet-name>
        <servlet-class>forAction.ticketInput</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ticketInput</servlet-name>
        <url-pattern>*.servletticketInput</url-pattern>
    </servlet-mapping>
    <!--工作票批量导出-->
    <servlet>
        <servlet-name>ticketMultiOutput</servlet-name>
        <servlet-class>forAction.ticketMultiOutput</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ticketMultiOutput</servlet-name>
        <url-pattern>*.servletticketMultiOutput</url-pattern>
    </servlet-mapping>
    <!--登录界面-->
    <servlet>
        <servlet-name>sign_in</servlet-name>
        <servlet-class>forAction.sign_in</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign_in</servlet-name>
        <url-pattern>*.sign_in</url-pattern>
    </servlet-mapping>
    <!--注册界面-->
    <servlet>
        <servlet-name>sign_up</servlet-name>
        <servlet-class>forAction.sign_up</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign_up</servlet-name>
        <url-pattern>*.sign_up</url-pattern>
    </servlet-mapping>
    <!--忘记密码界面-->
    <servlet>
        <servlet-name>resetPwd</servlet-name>
        <servlet-class>forAction.resetPwd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>resetPwd</servlet-name>
        <url-pattern>*.resetPwd</url-pattern>
    </servlet-mapping>

    <!--testChart-->
    <servlet>
        <servlet-name>testchart</servlet-name>
        <servlet-class>forAction.testChart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testchart</servlet-name>
        <url-pattern>*.testchart</url-pattern>
    </servlet-mapping>
</web-app>